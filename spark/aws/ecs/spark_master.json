{
    "containerDefinitions": [
    {
        "name": "spark_master",
        "image": "dtpc/sparkstat",
        "cpu": 10,
        "command": [
            "/start-master.sh"
        ],
        "portMappings": [
        {
            "hostPort": 8080,
            "containerPort": 8080,
            "protocol": "tcp"
        }
        ],
        "workingDirectory": "/etc/spark",
        "memory": 256,
        "essential": true
    }
    ],
    "family": "spark_revrand"
}
